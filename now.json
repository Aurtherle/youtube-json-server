{
    "version": 2,
    "builds": [
        { "src": "index.html", "use": "@now/static" },
        { "src": "playlist.js", "use": "@now/node" }
    ],
    "routes": [
        {
            "src": "/playlist/(?<playlistId>[^/]*)",
            "headers": {"cache-control": "s-maxage=300, stale-while-revalidate"},
            "dest": "/playlist.js?id=playlistId"
        }
    ]
}